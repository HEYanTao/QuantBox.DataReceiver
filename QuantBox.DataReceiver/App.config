<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <clear />
    <add key="DataPath" value="D:\test\Data"/>
    <add key="ConfigPath" value="D:\test"/>
    <!-- 交易部分，读取连接信息进行登录，然后将下载到的合约列表写入文件 -->
    <add key="TradeConnectionConfigFileName" value="TdConnectionConfig.json"/>
    <add key="TradeInstrumentInfoListFileName" value="TdInstrumentInfoList.json"/>
    <!-- 行情部分，读取连接信息进行登录，读取合约列表进行订阅，考虑需要排除的信息 -->
    <add key="MarketDataConnectionConfigListFileName" value="MdConnectionConfigList.json"/>
    <add key="MarketDataInstrumentInfoListFileName" value="MdInstrumentInfoList.json"/>
    <add key="MarketDataIncludeFilterListFileName" value="MdIncludeFilterList.json"/>
    <add key="MarketDataExcludeFilterListFileName" value="MdExcludeFilterList.json"/>
    <!-- TradeInstrumentInfoListFileName和MarketDataInstrumentInfoListFileName可以指向同一文件 -->
  </appSettings>
</configuration>